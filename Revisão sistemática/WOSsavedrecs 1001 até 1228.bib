Editor = {Vrontis, D and Weber, Y and Tsoukatos, E},
Title = {PROFITABILITY OF THE ITALIAN CREDIT COOPERATIVE BANKS},
Booktitle = {INNOVATION, ENTREPRENEURSHIP AND DIGITAL ECOSYSTEMS},
Year = {2016},
