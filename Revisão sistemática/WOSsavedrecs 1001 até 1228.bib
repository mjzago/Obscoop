@inproceedings{ WOS:000263151800160,
Author = {Luo Jianfeng and Pan An'e and Yang Qing},
Editor = {Duysters, G and Kaminishi, K},
Title = {Research on the Risk Early-warning Indicators System of the Rural Credit
   Cooperatives Based on the Entire Risk Management},
Booktitle = {PROCEEDINGS OF THE 5TH INTERNATIONAL CONFERENCE ON INNOVATION \&
   MANAGEMENT, VOLS I AND II},
Year = {2008},
Pages = {878-882},
Note = {5th International Conference on Innovation and Management, Maastricht,
   NETHERLANDS, DEC 10-11, 2008},
Abstract = {It is essential to construct the scientific risk early-warning
   indicators system in order to prevent and control the risk from Rural
   Credit Cooperatives (RCC). Based on the theory of Entire Risk Management
   (ERM) and the risk management experiences from the international
   financial institutions, combined with the real Situation of China Rural
   Credit Cooperatives, this paper designs risk early-warning indicators
   system aimed at RCC. This system is composed of capital adequacy risk,
   credit risk, liquidity risk, profitability risk, operational risk and
   marketing risk. Each layer of sub-risk is assessed by some specific
   indicators. Furthermore, this paper theoretically discusses the single
   indicator early-warning model and comprehensive risk early-warning
   model.},
