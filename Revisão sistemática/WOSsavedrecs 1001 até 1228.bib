@article{ WOS:A1992HW93800004,
Author = {FITZROY, FR and KRAFT, K},
Title = {FORMS OF PROFIT SHARING AND FIRM PERFORMANCE - THEORETICAL FOUNDATIONS
   AND EMPIRICAL PROBLEMS},
Journal = {KYKLOS},
Year = {1992},
Volume = {45},
Number = {2},
Pages = {209-225},
Abstract = {Most work on profit sharing has been empirical, and in a rare consensus,
   almost all studies have found a positive relationship between profit
   sharing and productivity. Theoretical foundations have been neglected in
   this work, and no attempt has been made to relate these empirical
   findings to extensive evidence for rent-sharing, or wage differentials
   related to profits that are hard to explain on the basis of unobserved
   individual characteristics. Here we argue that explicit and implicit
   forms of profit sharing should be viewed as two sides of the same coin,
   and can both be given convincing theoretical foundations. Relevant
   models include the importance of fairness and equitable pay and repeated
   games with mutual monitoring by profit or rent-sharing workers. Most
   empirical work has neglected the selection problem, or the existence of
   favourable but unobserved preconditions for profit sharing, and so
   probably exaggerates productivity gains from profit sharing in isolation
   and benefits from tax incentives. Cross-sectional regressions have also
   failed to uncover specific channels of influence of profit sharing.},
