@article{ WOS:000579610100001,
Author = {Shi, Zhanwen and Cao, Erbao},
Title = {Contract farming problems and games under yield uncertainty{*}},
Journal = {AUSTRALIAN JOURNAL OF AGRICULTURAL AND RESOURCE ECONOMICS},
