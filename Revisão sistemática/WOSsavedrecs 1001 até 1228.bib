@article{ WOS:000350117700007,
Author = {Arando, Saioa and Gago, Monica and Jones, Derek C. and Kato, Takao},
Title = {EFFICIENCY IN EMPLOYEE-OWNED ENTERPRISES: AN ECONOMETRIC CASE STUDY OF
   MONDRAGON},
