@article{ WOS:000429920500006,
Author = {Ifeanyi-Obi, C. C. and Iremesuk, P.},
Title = {Environmental Factors Influencing Artisanal Fishing in Eastern Obolo
   Local Government Area of Akwa Ibom State},
Journal = {JOURNAL OF AGRICULTURAL EXTENSION},
