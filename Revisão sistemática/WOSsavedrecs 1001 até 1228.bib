@article{ WOS:000285108700006,
Author = {Chakraborty, Debojyoti},
Title = {Small holder's carbon forestry project in Haryana India: issues and
   challenges},
Journal = {MITIGATION AND ADAPTATION STRATEGIES FOR GLOBAL CHANGE},
