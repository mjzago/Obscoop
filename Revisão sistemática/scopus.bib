@ARTICLE{Uetake201429,
	author = {Uetake, Tetsuya},
	title = {Agri-environmental Management Through Collective Action},
	year = {2014},
	journal = {EuroChoices},
	volume = {13},
	number = {3},
	pages = {29 – 34},
	doi = {10.1111/1746-692X.12072},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84912527202&doi=10.1111%2f1746-692X.12072&partnerID=40&md5=d3f9858e92c1ed05dbd07b5c119fa1ee},
	affiliations = {Economic Partnership Team, International Department, Ministry of Agriculture, Forestry and Fisheries of Japan, Tokyo, Japan; Organisation for Economic Co-operation and Development (OECD), Tokyo, Japan},
	abstract = {Agriculture has both positive and negative impacts on the environment. With the growing awareness of environmental issues, improved approaches to agri-environmental management have become important policy issues. Previous studies on agri-environmental policies have focused on individual farmers rather than on collective action. However, some agri-environmental issues need to be managed by farmers co-operatively. Collective action can facilitate geographically appropriate management of resources, allow for shared knowledge among members thus increasing their capacities, and provide solutions for dealing with agri-environmental issues. However, sometimes, transaction costs can hinder collective action from being initiated. Moreover, farmers often need external support such as scientific knowledge, technical information and financial assistance. Thus, external help from public agencies or other interested bodies may be necessary to promote collective action. Many local and central governments encourage farmers and other stakeholders to take initiatives to manage the environment by themselves through various policy measures including technical assistance and financial assistance programmes. Collaboration with intermediaries is also necessary to facilitate state-community relationships. Governments can assist agri-environmental management, as opposed to interfering with it, through better agri-environmental policies that promote collective action. L'agriculture a des effets à la fois positifs et négatifs sur l'environnement. Les questions environnementales se faisant de plus en plus présentes, il devient important d'améliorer la façon dont les politiques approchent la gestion agroenvironnementale. Jusqu'à présent, les études sur les politiques agroenvironnementales se sont intéressées aux agriculteurs individuels plutôt qu'aux actions collectives. Certaines questions environnementales demandent cependant une coopération entre agriculteurs. L'action collective peut faciliter la gestion des ressources à un niveau géographique adéquat, permettre de partager des connaissances entre membres, et donc améliorer leur capacité, et offrir des solutions pour régler les questions environnementales. Cependant, les coûts de transaction peuvent parfois empêcher l'émergence d'actions collectives. En outre, les agriculteurs ont souvent besoin d'une aide extérieure comme des connaissances scientifiques, des informations techniques et de l'aide financière. Une aide des agences publiques ou d'autres institutions intéressées peut être nécessaire pour promouvoir l'action collective. De nombreux pouvoirs publics centraux et locaux encouragent les agriculteurs et d'autres parties prenantes à prendre des initiatives pour gérer l'environnement par eux-mêmes par le biais de diverses mesures gouvernementales comme l'assistance technique et les programmes d'aide financière. La collaboration avec des intermédiaires est également nécessaire pour faciliter les relations entre pouvoirs publics et communautés. Les pouvoirs publics peuvent aider la gestion environnementale plutôt qu'interférer avec elle, en mettant en place des mesures agroenvironnementales qui promeuvent l'action collective. Die Landwirtschaft hat sowohl positive als auch negative Auswirkungen auf die Umwelt. Mit zunehmender Wahrnehmung von Umweltbelangen hat die Verbesserung der Ansätze zum Agrarumweltmanagement einen hohen politischen Stellenwert erreicht. Bisherige Untersuchungen zu Agrarumweltpolitiken konzentrierten sich eher auf einzelne landwirtschaftliche Betriebe als auf ein kollektives Vorgehen. Einige Umweltbelange erfordern allerdings die Zusammenarbeit der Landwirte untereinander. Ein kollektives Vorgehen kann eine geografisch sinnvolle Handhabung von Ressourcen erleichtern, die Weitergabe von Wissen untereinander fördern und somit zu einer höheren Leistungsfähigkeit führen sowie zur Lösungsfindung bei Umweltfragen beitragen. In einigen Fällen kann es jedoch vorkommen, dass es auf Grund von Transaktionskosten nicht zu einem kollektiven Vorgehen kommt. Zudem benötigen die Landwirte oftmals Unterstützung von außen wie z.B. Kenntnisse aus Wissenschaft und technische Informationen oder Finanzhilfen. Daher könnte Hilfe von außen seitens staatlicher Stellen oder anderer Einrichtungen des öffentlichen Interesses erforderlich sein, um ein kollektives Vorgehen zu fördern. Viele Regional- und Zentralregierungen ermutigen die Landwirte und weitere Akteure, die Initiative zu ergreifen und sich der Umweltbelange selbst mit Hilfe von verschiedenen Politikmaßnahmen sowie technischer Unterstützung und Programmen zur Finanzierungshilfe anzunehmen. Die Zusammenarbeit mit Vermittlern ist ebenfalls erforderlich, um die Beziehungen zwischen Staat und Kommunen zu fördern. Die Regierungen können das Agrarumweltmanagement unterstützen, anstatt hier störend einzugreifen, indem sie bessere Agrarumweltpolitiken schaffen, die ein kollektives Vorgehen fördern. Agriculture has both positive and negative impacts on the environment. With the growing awareness of environmental issues, improved approaches to agri-environmental management have become important policy issues. Previous studies on agri-environmental policies have focused on individual farmers rather than on collective action. However, some agri-environmental issues need to be managed by farmers co-operatively. Collective action can facilitate geographically appropriate management of resources, allow for shared knowledge among members thus increasing their capacities, and provide solutions for dealing with agri-environmental issues. Copyright © 2014 The Agricultural Economics Society and the European Association of Agricultural Economists 13 3 December 2014 10.1111/1746-692X.12072 Original Article Original Articles © 2014 The Author EuroChoices © 2014 The Agricultural Economics Society and the European Association of Agricultural Economists.},
	issn = {14780917},
	language = {English},
