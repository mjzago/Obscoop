	title = {An integrated conceptual framework for the study of agricultural cooperatives: from repolitisation to cooperative sustainability},
	year = {2020},
	journal = {Journal of Rural Studies},
	volume = {78},
	pages = {467 – 479},
	doi = {10.1016/j.jrurstud.2020.06.019},
