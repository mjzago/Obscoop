	affiliations = {Baghabarighat Dairy Plant, Milk Vita, Shahjadpur, Sirajgong, Bangladesh; Department of Animal Breeding in the Tropics and Sub-tropics, Humboldt University of Berlin, Germany; IFCN Dairy Research Center at the Department of Agricultural Economics, University of Kiel, 116, 24118 Kiel, Schauenburger Str, Germany; Department of Dairy Science, Bangladesh Agricultural University, Mymensingh-2202, Bangladesh},
